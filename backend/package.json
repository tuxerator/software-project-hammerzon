{
    "name": "softwareproject",
    "version": "1.0.0",
    "description": "Template for the softwareproject lecture at the University of Konstanz, Germany",
    "scripts": {
        "build": "tsc && tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "start": "npm run build && node dist/src/server.js",
        "lint": "eslint \"**/*.{js,ts}\"",
        "test": "npm run build-ts && jasmine --config=jasmine.json",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"nodemon --inspect dist/src/server.js\""
    },
    "dependencies": {
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "cookie-parser": "^1.4.6",
        "cors": "2.8.5",
        "csv": "^6.1.0",
        "csv-parser": "^3.0.0",
        "errorhandler": "1.5.1",
        "express": "4.17.3",
        "express-session": "^1.17.2",
        "express-sessions": "^1.0.6",
        "fs": "^0.0.1-security",
        "mongoose": "^6.3.1",
        "multer": "^1.4.4",
        "pg": "^8.7.3",
        "pg-hstore": "^2.3.4",
        "source-map-support": "0.5.21",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "2.8.12",
        "@types/errorhandler": "1.5.0",
        "@types/eslint": "8.4.1",
        "@types/express": "4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jasmine": "4.0.1",
        "@types/multer": "^1.4.7",
        "@types/source-map-support": "0.5.4",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "concurrently": "7.0.0",
        "eslint": "8.12.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "jasmine": "4.0.2",
        "nodemon": "2.0.15",
        "supertest": "6.2.2",
        "typescript": "4.6.3"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
